<h1>User Login</h1>
<form action="<%= session_url %>" method="POST">
    <label>Username
        <input type="text" name="sessions[username]">
    </label>
    <br>
    <label>Password
        <input type="password" name="sessions[password]">
    </label>
    <br>
    <input type="submit" value="Log In">
</form>
